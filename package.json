{
  "name": "webpack-typescript-starter",
  "version": "0.0.1",
  "description": "A simple Webpack 2 starter with TypeScript transpilation",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --port 9000 --inline --progress --profile --colors --watch --content-base src/",
    "build": "webpack --config webpack.config.js",
    "build.prod": "webpack --config webpack.config.js -p",
    "test-1": "karma start test-conf/karma.config.1.js",
    "test-2": "karma start test-conf/karma.config.2.js",
    "test-3": "karma start test-conf/karma.config.3.js",
    "test-4": "karma start test-conf/karma.config.4.js",
    "test-5": "karma start test-conf/karma.config.5.js",
    "test-6": "karma start test-conf/karma.config.6.js",
    "test-7": "karma start test-conf/karma.config.7.js",
    "test-8": "karma start test-conf/karma.config.8.js",
    "test-all": "karma start test-conf/karma.config.all.js"
  },
  "keywords": [
    "webpack",
    "webpack2",
    "typescript"
  ],
  "author": "Juri Strumpflohner",
  "license": "ISC",
  "devDependencies": {
    "@types/jasmine": "2.5.40",
    "@types/node": "7.0.0",
    "awesome-typescript-loader": "3.0.0-beta.18",
    "jasmine-core": "2.5.2",
    "karma": "1.4.0",
    "karma-jasmine": "1.1.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-webpack": "2.0.1",
    "source-map-loader": "0.1.6",
    "tslint": "4.3.1",
    "tslint-loader": "3.3.0",
    "typescript": "^2.4.1",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.2.1"
  },
  "dependencies": {
    "@types/jquery": "^3.2.5"
  }
}
